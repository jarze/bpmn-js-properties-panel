{
  "extends": [
    "plugin:bpmn-io/browser",
    "plugin:bpmn-io/jsx",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "import/first": "error",
    "import/no-amd": "error",
    "import/no-webpack-loader-syntax": "error",
    "react-hooks/exhaustive-deps": "off",
    "array-bracket-spacing": "off",
    "indent": false,
    "no-undef": "error",
    "no-label-var": "error"
  },
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "es6": true
  }
}